{
  "name": "diaryx",
  "version": "0.1.0",
  "description": "",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "tauri": "tauri",
    "docs": "typedoc",
    "docs:serve": "typedoc && cd docs && python3 -m http.server 8080",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "docs:all": "bun run docs && bun run build-storybook",
    "docs:serve-all": "bun run docs:all && echo 'TypeDoc: http://localhost:8080' && echo 'Storybook: http://localhost:8081' && cd storybook-static && python3 -m http.server 8081 & cd docs && python3 -m http.server 8080",
    "docs:deploy": "bun run docs:all && mkdir -p docs-site && cp -r docs/* docs-site/ && cp storybook-static/index.html docs-site/storybook.html"
  },
  "license": "MIT",
